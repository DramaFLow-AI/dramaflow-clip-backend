{
  "name": "gemini",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build && copyfiles package.json .env.prod .env.dev .env.staging sa.json ecosystem.config.js start.sh fix-scripts.sh dist && copyfiles -f prisma/schema.prisma dist/prisma",
    "build:staging": "nest build && copyfiles package.json .env.staging sa.json ecosystem.config.js start.sh fix-scripts.sh dist && copyfiles -f prisma/schema.prisma dist/prisma",
    "build:prod": "nest build && copyfiles package.json .env.prod sa.json ecosystem.config.js start.sh fix-scripts.sh dist && copyfiles -f prisma/schema.prisma dist/prisma",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:staging": "cross-env NODE_ENV=staging node src/main",
    "start:prod": "cross-env NODE_ENV=prod node src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "swagger": "tsx script/generate-swagger.ts",
    "prisma:generate": "dotenv -e .env.dev npx prisma generate",
    "prisma:generate:prod": "dotenv -e .env.prod npx prisma generate",
    "prisma:pull": "dotenv -e .env.dev npx prisma db pull",
    "prisma:studio": "dotenv -e .env.dev npx prisma studio",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "@google-cloud/text-to-speech": "^6.4.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/genai": "^1.10.0",
    "@nestjs/bull": "^11.0.3",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
