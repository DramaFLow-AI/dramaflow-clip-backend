    "@prisma/client": "^6.13.0",
    "@scalar/nestjs-api-reference": "^1.0.8",
    "@smithy/node-http-handler": "^4.1.1",
    "add": "^2.0.6",
    "ali-oss": "^6.23.0",
    "body-parser": "^2.2.0",
    "bull": "^4.16.5",
    "bullmq": "^5.56.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cross-env": "^10.1.0",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.0",
    "dotenv-cli": "^10.0.0",
    "express": "^5.1.0",
    "google-auth-library": "^10.4.0",
    "https-proxy-agent": "^7.0.6",
